{
	"parserOptions": {
		"sourceType": "script"
	},
	"env": {
		"browser": true,
		"node": false
	},
	"plugins": [
		"compat"
	],
	"extends": [
		"eslint:recommended",
		"plugin:@wordpress/eslint-plugin/recommended"
	],
	"rules": {
		"compat/compat": "warn",
		"camelcase": "off",
		"no-warning-comments": "warn",
		"strict": ["error", "never"]
	}
}
